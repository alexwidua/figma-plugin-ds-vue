{
	"name": "figma-plugin-ds-vue",
	"version": "1.2.0",
	"description": "Vue component library that matches the Figma Design System for building Figma plugins.",
	"private": false,
	"main": "./dist/figma-plugin-ds-vue.umd.js",
	"style": "./dist/figma-plugin-ds-vue.css",
	"scripts": {
		"serve": "vue-cli-service serve src/serve.js --mode development",
		"build": "vue-cli-service build src/serve.js --dest demo --no-clean",
		"build-lib": "vue-cli-service build --target lib --name figma-plugin-ds-vue src/build.js",
		"prepublishOnly": "npm run build-lib"
	},
	"dependencies": {},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"babel-eslint": "^10.1.0",
		"core-js": "^3.6.5",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"sass": "^1.26.5",
		"sass-loader": "^8.0.2",
		"vue": "^2.6.11",
		"vue-template-compiler": "^2.6.11"
	},
	"peerDependencies": {
		"vue": "^2.6.11"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alexwidua/figma-plugin-ds-vue.git"
	},
	"files": [
		"dist/*"
	],
	"keywords": [
		"vue",
		"figma",
		"plugin",
		"ds",
		"design",
		"system",
		"ui",
		"component",
		"library"
	],
	"author": "Alexander Widua",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/alexwidua/figma-plugin-ds-vue/issues"
	},
	"homepage": "https://github.com/alexwidua/figma-plugin-ds-vue#readme"
}
